{
  "name": "trackdo",
  "private": true,
  "type": "module",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "dev:lan": "vite --host 0.0.0.0",
    "build": "vite build",
    "preview": "vite preview",
    "lint": "eslint . --fix",
    "backend:dev": "cd backend && npm run dev",
    "backend:dev:lan": "cd backend && npm run dev:lan",
    "backend:install": "cd backend && npm install",
    "backend:start": "cd backend && npm start",
    "dev:full": "concurrently \"npm run backend:dev\" \"npm run dev\"",
    "dev:full:lan": "concurrently \"npm run backend:dev:lan\" \"npm run dev:lan\"",
    "install:all": "npm install && npm run backend:install",
    "db:setup": "node backend/migrations/setup-database.js",
    "db:migrate": "node backend/migrations/setup-database.js"
  },
  "dependencies": {
    "@fontsource/roboto": "5.2.5",
    "@mdi/font": "7.4.47",
    "@schedule-x/calendar": "^2.36.0",
    "@schedule-x/theme-default": "^2.36.0",
    "@schedule-x/vue": "^2.34.0",
    "axios": "^1.6.2",
    "v-calendar": "^3.1.2",
    "vue": "^3.5.13",
    "vuetify": "^3.8.1"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^5.2.3",
    "concurrently": "^9.2.0",
    "eslint": "^9.23.0",
    "eslint-config-vuetify": "^3.0.3",
    "globals": "^16.0.0",
    "pinia": "^3.0.1",
    "sass-embedded": "^1.86.3",
    "unplugin-auto-import": "^19.1.1",
    "unplugin-fonts": "^1.3.1",
    "unplugin-vue-components": "^28.4.1",
    "unplugin-vue-router": "^0.12.0",
    "vite": "^6.2.2",
    "vite-plugin-vue-layouts-next": "^0.0.8",
    "vite-plugin-vuetify": "^2.1.1",
    "vue-router": "^4.5.0"
  }
}
